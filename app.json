{
  "expo": {
    "name": "MyNativeApp",
    "slug": "MyNativeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/coffee-icon.png",
    "scheme": "mynativeapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/coffee-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "d147a0fe-6e1a-4b54-9ce8-b6416e1ea36e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d147a0fe-6e1a-4b54-9ce8-b6416e1ea36e"
    }
  }
}
